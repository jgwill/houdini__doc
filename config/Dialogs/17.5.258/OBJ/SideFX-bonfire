			LAYOUT(horizontal)  HSTRETCH;

EmitColor_Color.multiramp = 
{
    LAYOUT(vertical) STRETCH

    ROW
    {
	LOOK(plain) JUSTIFY(left,center) SPACING(0.1) VMARGIN(0.02) HSTRETCH

	COL
	{
	    JUSTIFY(right,center) WIDTH(1.5)
	    LABEL "Point No.";
	}

	EmitColor_Color.pointno.gad
	    = INT_SLIDER_FIELD VALUE(EmitColor_Color.pointno.val) HSTRETCH;
    }
}

EmitColor_Color.buttons.gad = COL
{
    VSTRETCH

    ICON_BUTTON_STRIP
    {
	LAYOUT(horizontal) INITSIZE(20p, 20p) 

	ACTION_ICONBUTTON "BUTTONS_delete"
		HELP("Remove Point") VALUE(EmitColor_Color.delpoint.val);
	ACTION_ICONBUTTON "BUTTONS_add"
		HELP("Add Point") VALUE(EmitColor_Color.addpoint.val);
    }

    { VSTRETCH }

    EmitColor_Color.toggleparms.gad
      = ACTION_ICONBUTTON "BUTTONS_show_controls"
	HELP("Hide Controls")
	VALUE(EmitColor_Color.toggle.val)
	SENDSKEY("h.pane.parms.toggle_ramp_parms")
	LOOKFLAT UI_FLAT_ICON_BTN_SIZE_TINY;
}

EmitColor_Color.parm =
{
    LAYOUT(vertical) HSTRETCH JUSTIFY(left, center)

    EmitColor_Color.ramp.gad = ROW
    {
	MARGIN(0.02) SPACING(0.02) JUSTIFY(left, top) HSTRETCH

	EmitColor_Color.buttons.gad VSTRETCH;

	EmitColor_Color.si_rampedit HSTRETCH;
    }
}

EmitColor_Color.gad =
{
    LAYOUT(vertical) MARGIN(2p) HSTRETCH

    {
	LAYOUT(horizontal) HSTRETCH

	PARM_LABEL_SIMPLE("Color", "EmitColor_Color");

	{ HSTRETCH }

    }

    {
	LAYOUT(vertical) MARGIN(2p) SPACING(0.02) LOOK(lightline) HSTRETCH

	EmitColor_Color.parm;

	SWITCHER tight
	{
	    LAYOUT(overlap) VALUE(EmitColor_Color.hideparms.val) HSTRETCH
	    EmitColor_Color.multiramp;
	    {}
	}
    }
}

